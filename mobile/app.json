{
  "expo": {
    "name": "SatsConnect",
    "slug": "satsconnect",
    "version": "0.1.0",
    "scheme": "satsconnect",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.satsconnect.wallet"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store"
    ]
  }
}
